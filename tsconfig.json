{
  "compilerOptions": {
      "outDir": "./dist",
      "target": "ES5",
      "module": "CommonJS",//组织代码方式
      "sourceMap": true,
      "moduleResolution": "Node", // 模块解决策略
      "experimentalDecorators": true, // 开启装饰器定义
      "allowSyntheticDefaultImports": true, // 允许es6方式import
      "lib": ["ES2021", "DOM"],
      "typeRoots": ["./node_modules/@types"],
  },
  "include": ["src/**/*.ts"],
  "typedocOptions": {
    "name": "Auto I18n",
    "excludeExternals": true,
    "excludePrivate": true,
    "readme": "./README.md",
    "out": "docs/"
 }
}